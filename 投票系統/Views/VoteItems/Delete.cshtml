@model VoteOnline.Models.VoteItem

@{
    ViewData["Title"] = "Delete";
}



<h3>確認要刪除此投票項目?</h3>
<div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            項目
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ItemName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="VoteItemId" />
        <input type="submit" value="刪除" class="btn btn-danger" /> |
        <a asp-action="Index">上一頁</a>
    </form>
</div>
